{
  "name": "furious/skeleton",
  "type": "project",
  "license": "MIT",
  "require": {
    "php" : ">=7.4",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/container": "^1.0",
    "furious/router": "1.0.*",
    "furious/psr15": "^1.0",
    "furious/container": "^1.0",
    "furious/http-runner": "^0.1",
    "symfony/console": "^4.1",
    "ramsey/uuid": "^4.0",
    "webmozart/assert": "^1.9",
    "league/oauth2-server": "^8.1",
    "laminas/laminas-diactoros": "^2.3",
    "doctrine/migrations": "^1.8",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5"
